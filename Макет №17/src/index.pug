    //- Попап входа в аккаунт
    .popup.login-popup
      .popup__content.popup-content
        button.popup-content__close(type="button")
          svg(width="21" height="21" viewBox="0 0 21 21" fill="none" xmlns="http://www.w3.org/2000/svg")
            path(d="M20 1L1 20" stroke="white" stroke-linecap="round" stroke-linejoin="round")
            path(d="M1 1L20 20" stroke="white" stroke-linecap="round" stroke-linejoin="round")

        h3.popup-content__title Вход
        form.popup-content__form.popup-form.login-form(name="login" method="#" action="#")
          input.popup-form__input(type="email" placeholder="Е-маил")
          input.popup-form__input(type="password" placeholder="Пароль")
          button.popup-form__submit(type="submit") Войти
        a.popup-content__link(href="#") Зарегистрироваться

    //- Шапка сайта
    header.header#header
      .container.header__container
        .header__row
