      nav.nav(class="wow animate__animated animate__fadeIn" data-wow-duration="1s")
        .nav__container.container
          .nav__row
            .nav__logo.logo
              a.logo__link(href="#")
                img(src="./images/logo.svg" alt="logo")
                span.text Healthy switcher
            
            ul.nav__list.nav-list
              li.nav-list__elem
                a.nav-list__link(href="#works") Этапы
              li.nav-list__elem
                a.nav-list__link(href="#recipes") Рецепты
              li.nav-list__elem
